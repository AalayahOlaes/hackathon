{
  "manifest_version": 3,
  "name": "Netflix-Searcher",
  "description": "Find Movies From certain Genres",
  "version": "2.0",
  "page_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": ["tabs"]
}
